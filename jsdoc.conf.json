{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["./"],
		"exclude": ["./node_modules", "./jsdoc"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [
		"plugins/markdown"
	],
	"markdown":{
		"tags": ["requires"]
	},
	"templates": {
		"applicationName": "PointerEvent",
		"disqus": "",
		"googleAnalytics": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
		"linenums": true
	},
	"opts": {
		"readme":		"./README.md",
		"template":		"templates/jaguarjs-jsdoc",
		"encoding":		"utf8",
		"destination":	"./jsdoc",
		"recurse": 		true,
		"private":		true
	}
}